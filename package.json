{
  "name": "nha-automation",
  "version": "1.0.0",
  "description": "NHA Scraper Executable",
  "main": "scriptv3.js",
  "bin": "scriptv3.js",
  "scripts": {
    "prebuild:exe": "del-cli node_modules/typed-query-selector/shim.d.ts",
    "build:exe": "npm run prebuild:exe && pkg script.js --targets node18-macos-x64 --public"
  },
  "pkg": {
    "ignore": [
      "**/*.d.ts"
    ]
  },
  "dependencies": {
    "pkg": "^5.8.1",
    "puppeteer-core": "^21.5.0"
  },
  "devDependencies": {
    "del-cli": "^5.1.0"
  }
}
